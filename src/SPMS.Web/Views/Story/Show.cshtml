@model SPMS.ViewModel.Story.StoryPostViewModel

<div class="card">
    <div class="card-header">
        <h4>@Model.Title</h4>
        <p>@Model.Location  @Model.Timeline</p>
    </div>
    <div class="card-body">
        <markdown markdown="@Model.Content" />
    </div>
    <div class="card-footer">
        @Model.PublishedAt by
    </div>
</div>