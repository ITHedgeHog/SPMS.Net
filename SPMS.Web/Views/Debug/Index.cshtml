@using System.Collections
@{
    ViewData["Title"] = "Debug Variables";
}
<h1>Claims</h1>

<ul>
    @foreach (var claim in User.Claims)
    {
        <li>@claim.Type - @claim.Value</li>
    }
</ul>

<ul>

    @{
        var envs = Environment.GetEnvironmentVariables();
        foreach (DictionaryEntry e in envs)
        {
            <li>@e.Key - @e.Value</li>
        }

    }
</ul>